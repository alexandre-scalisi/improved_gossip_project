<div class="container-fluid m-4 p-4">
  <h1>Se connecter</h1><br>
  <%= form_tag url_for(action: 'create'), method: "post" do %>

  <%= label_tag 'email ' %><br />
  <%= text_field_tag 'email' %>
  <br /> <br />

  <%= label_tag 'Password ' %><br />
  <%= password_field_tag 'password'%>
  <br /> <br />

  <%= submit_tag "Se connecter" %>

  <% end %>
</div>
